<h1>Create your new character here!</h1>

    
	<%= form_with scope: :character, url: characters_path, local: true do |f| %>
	    <p>
            <%= f.label :first_name %> <br>
            <%= f.text_field :first_name %>
        </p>

        <p>
            <%= f.label :last_name %>  <br>
            <%= f.text_field :last_name %>
        </p>

        <p>
	        <%= f.submit %>
        </p>
	<% end %>